<svg width="64" height="64" viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg">
  <!-- Terminal window background -->
  <rect x="4" y="4" width="56" height="56" rx="6" ry="6" fill="#1e1e1e" stroke="#333" stroke-width="2"/>
  
  <!-- Title bar -->
  <rect x="4" y="4" width="56" height="12" rx="6" ry="6" fill="#333"/>
  <rect x="4" y="10" width="56" height="6" fill="#333"/>
  
  <!-- Window controls -->
  <circle cx="12" cy="10" r="2" fill="#ff5f56"/>
  <circle cx="20" cy="10" r="2" fill="#ffbd2e"/>
  <circle cx="28" cy="10" r="2" fill="#27ca3f"/>
  
  <!-- Terminal prompt -->
  <text x="8" y="28" font-family="monospace" font-size="8" fill="#00ff00">$</text>
  
  <!-- Command line -->
  <text x="14" y="28" font-family="monospace" font-size="6" fill="#ffffff">claude --help</text>
  
  <!-- Output lines -->
  <rect x="8" y="32" width="24" height="2" fill="#00ff00" opacity="0.7"/>
  <rect x="8" y="36" width="18" height="2" fill="#00ff00" opacity="0.5"/>
  <rect x="8" y="40" width="30" height="2" fill="#00ff00" opacity="0.3"/>
  
  <!-- Security shield -->
  <path d="M40 24 L48 24 L48 32 L44 36 L40 32 Z" fill="#ffd700" stroke="#333" stroke-width="1"/>
  <path d="M42 26 L46 26 L46 30 L44 32 L42 30 Z" fill="#333"/>
  
  <!-- Lock icon -->
  <rect x="50" y="46" width="8" height="6" rx="1" fill="#ffd700"/>
  <path d="M52 46 L52 44 C52 42 53 41 54 41 C55 41 56 42 56 44 L56 46" stroke="#ffd700" stroke-width="1" fill="none"/>
</svg>
